#!/usr/bin/env bash 

LIB_DIR="${HOME}/confeez/lib"

print_command() {
  command="$1"
  local DEFAULT_COLOR=$(tput sgr0)
  local GREY_COLOR=$(tput setaf 245)
  echo $GREY_COLOR"${command}"$DEFAULT_COLOR
}

# SYSTEM ---------------------------------------------------------------------------------------------------------------------------------
source "${LIB_DIR}"/system.sh

# FILE SYSTEM ----------------------------------------------------------------------------------------------------------------------------
source "${LIB_DIR}"/file-system.sh

# GREP -----------------------------------------------------------------------------------------------------------------------------------
